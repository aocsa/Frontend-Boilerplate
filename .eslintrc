{
"parser": "babel-eslint",
"extends": ["airbnb"],
"rules": {
    "react/jsx-filename-extension": 0,
    "import/prefer-default-export": 0,
    "max-len": 0,
    "react/jsx-no-bind": 0,
    "react/jsx-indent": 0,
    "react/jsx-indent-props": 0,
    "react/prop-types": 0,
    "import/no-extraneous-dependencies": 0
    }
}
