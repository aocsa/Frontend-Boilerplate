{
  "name": "frontend-boilerplate",
  "version": "1.0.0",
  "repository": "https://github.com/Babazon/Frontend-Boilerplate.git",
  "description": "A boilerplate for developing production ready front ends using React, Redux, React Router, Webpack, Chai, Mocha, Eslint etc..",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "NODE_ENV=production npm run clean && webpack",
    "dev": "NODE_ENV=development webpack-dev-server",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register  --require ./test/requireHacker.js babel-polyfill ./test/test_helper.js 'test/**/*.js'",
    "test:watch": "NODE_ENV=test npm run test -- --watch",
    "commit": "npm run clean && git add . && git commit -m 'Commit by commit script automatically. Do not worry about the contents. Probably a semi colon fix or a curly bracket delete here and there.' && git push origin master",
    "surge": "npm run build && surge -p dist",
    "amazon": "npm run build && s3-website deploy dist",
    "github": "git checkout gh-pages && npm run build && git subtree push --prefix dist origin gh-pages && git checkout master",
    "heroku": "npm un express && npm i express && git push heroku master && npm un express && npm i --save-dev express",
    "aws": "npm un express && npm i express && eb setenv NODE_ENV=production && eb deploy && npm un express && npm i --save-dev express && eb open "
  },
  "keywords": [
    "react",
    "react-router",
    "router",
    "redux",
    "react-redux",
    "webpack",
    "webpack3",
    "react-router4",
    "eslint",
    "mocha"
  ],
  "engines": {
    "node": "8.4.0"
  },
  "author": "Bas",
  "license": "ISC",
  "dependencies": {
    "react": "^16.0.0-rc.2",
    "react-dom": "^16.0.0-rc.2",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-form": "^7.0.4",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "chai-jquery": "^2.0.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "expect": "^21.0.0",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.4.2",
    "jquery": "^3.2.1",
    "jsdom": "^8.5.0",
    "mocha": "^3.5.0",
    "require-hacker": "^3.0.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.6",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.7.1"
  }
}
